{
  "name": "pokerface",
  "version": "0.1.0",
  "description": "Node.js HTTP gateway",
  "main": "index.js",
  "bin": "./cli.js",
  "scripts": {
    "build": "npx rollup -c",
    "watch": "npx rollup -cw",
    "prepare": "npm run build",
    "test": "npx jest",
    "run:server": "npx ts-node ./src/index.ts",
    "run:test-server": "npx ts-node ./test/test-servers.ts"
  },
  "keywords": [
    "gateway",
    "api",
    "proxy",
    "http",
    "https",
    "http2"
  ],
  "author": "Sandro Lain",
  "license": "MIT",
  "dependencies": {
    "fastify": "^2.14.0",
    "http-proxy": "^1.18.0",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "ts-interface-checker": "^0.1.10",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/http-proxy": "^1.17.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/pino": "^6.0.0",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "jest": "^25.5.1",
    "rollup": "^2.7.3",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-interface-builder": "^0.2.1",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
